<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="testbiao.example.demo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <android:use-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:use-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:name=".utils.ResourceManager"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Fail"
            android:label="@string/title_activity_fail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PasswordLogin"
            android:label="@string/title_activity_password_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Sucess"
            android:label="@string/title_activity_sucess"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>